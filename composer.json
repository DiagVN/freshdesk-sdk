{
    "name": "diagvn/freshdesk-sdk",
    "description": "PHP SDK for the Freshdesk API (v2)",
    "type": "library",
    "homepage": "https://github.com/DiagVN/freshdesk-sdk",
    "keywords": ["freshdesk", "customer service", "support", "help desk", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bradley Nguyen",
            "email": "giang.nguyen@diag.vn"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": ">=4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "evert/phpdoc-md" : "~0.2.0"
    },
    "autoload": {
        "psr-4": {"DiagVN\\Freshdesk\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Freshdesk\\tests\\": "tests/" }
    }
}
